{
  "name": "@ianwalter/dist",
  "version": "5.0.0",
  "description": "Simplify the generation of distribution files for your JavaScript library",
  "main": "dist/dist.js",
  "author": "Ian Walter <public@iankwalter.com> (https://iankwalter.com)",
  "license": "SEE LICENSE IN LICENSE",
  "keywords": [
    "esm",
    "bundle",
    "commonjs",
    "cjs",
    "mjs",
    "inline",
    "browser",
    "distribution",
    "rollup"
  ],
  "homepage": "https://github.com/ianwalter/dist#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ianwalter/dist.git"
  },
  "bugs": {
    "url": "https://github.com/ianwalter/dist/issues"
  },
  "bin": {
    "dist": "cli.js"
  },
  "files": [
    "cli.js",
    "index.js",
    "dist"
  ],
  "scripts": {
    "lint": "eslint index.js cli.js tests",
    "pretest": "./cli.js && ./cli.js",
    "test": "bff"
  },
  "dependencies": {
    "@babel/core": "^7.4.5",
    "@ianwalter/cli": "^1.1.1",
    "@ianwalter/fs": "^1.1.0",
    "@ianwalter/npm-short-name": "^1.0.0",
    "@ianwalter/print": "^2.3.1",
    "@ianwalter/rollup-plugin-hashbang": "^1.3.0",
    "builtin-modules": "^3.1.0",
    "p-settle": "^3.1.0",
    "read-pkg-up": "^6.0.0",
    "require-from-string": "^2.0.2",
    "rollup": "^1.15.5",
    "rollup-plugin-babel": "^4.3.2",
    "rollup-plugin-commonjs": "^10.0.0",
    "rollup-plugin-json": "^4.0.0",
    "rollup-plugin-node-resolve": "^5.0.3"
  },
  "devDependencies": {
    "@babel/runtime": "^7.4.5",
    "@ianwalter/babel-preset-lib": "^2.0.2",
    "@ianwalter/bff": "^5.2.1",
    "@ianwalter/eslint-config": "^2.0.1",
    "@ianwalter/renovate-config": "^1.1.0",
    "execa": "^1.0.0",
    "ky": "^0.10.0",
    "np": "^5.0.3",
    "postcss": "^7.0.17",
    "rollup-plugin-vue": "^5.0.0",
    "vue-template-compiler": "^2.6.10"
  },
  "babel": {
    "env": {
      "test": {
        "presets": [
          "@ianwalter/lib"
        ]
      }
    }
  },
  "eslintConfig": {
    "root": true,
    "extends": [
      "@ianwalter"
    ]
  },
  "eslintIgnore": [
    "test/tmp"
  ],
  "renovate": {
    "extends": [
      "@ianwalter"
    ]
  }
}
